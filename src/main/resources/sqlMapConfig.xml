<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--自动给类起名,默认为类名,且不分大小写-->
    <typeAliases>
        <package name="com.tangbaobao.pojo"/>
    </typeAliases>
    <!--配置Mapper.xml和Mapper映射接口-->
    <mappers>
        <package name="com.tangbaobao.dao"/>
    </mappers>

</configuration>